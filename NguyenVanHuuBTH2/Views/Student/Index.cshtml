@model IEnumerable<NguyenVanHuuBTH2.Models.Student>
    <P>
        <a class="btn btn-success" asp-action="Create">Create new student</a>
    </P>
    <table class="table table-all">
        <tr>
            <th>StudentID</th>
            <th>StudentName</th>
        </tr>
    @foreach (var item in Model)
    {
      <tr>
        <td>@Html.DisplayFor(m => item.StudentID)</td>
        <td>@Html.DisplayFor(m => item.StudentName)</td>
      </tr>  
    }
    </table>